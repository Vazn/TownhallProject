<div id="infosContainer">
   <iframe 
      src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d11648.10753898619!2d2.2742057920522067!3d43.124961427529186!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x12afcd67ae77d8c3%3A0x6f466cdf1bff05cb!2s11250%20Rouffiac-d&#39;Aude!5e0!3m2!1sfr!2sfr!4v1651748796360!5m2!1sfr!2sfr" 
      width="800" 
      height="450" 
      style="border:0;" 
      allowfullscreen="" 
      loading="lazy" 
      referrerpolicy="no-referrer-when-downgrade">
   </iframe>

   <article id="hoursSection">
      {{#each data.informations}}

         <header> 
            <h3 style="display:none">{{title}}</h3>
            <img class="picture" src="/uploads/images/{{thumbnail}}">
         </header>

         <pre>{{content}}</pre>
      {{/each}}
   </article>
</div>